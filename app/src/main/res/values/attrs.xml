<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ErrorView">
        <attr name="error_description" format="string" />
        <attr name="error_title" format="string" />
    </declare-styleable>
    <declare-styleable name="FocusView">
        <attr name="focusPaddingHorizontal" format="dimension" />
        <attr name="focusPaddingTop" format="dimension" />
        <attr name="focusPaddingBottom" format="dimension" />
        <attr name="focusBorderRadius" format="dimension" />
        <attr name="dimColor" format="color" />
