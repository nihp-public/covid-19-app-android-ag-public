    </declare-styleable>
    <declare-styleable name="StatusOptionView">
        <attr name="optionIcon" format="reference" />
        <attr name="optionIconBackgroundColor" format="reference"/>
        <attr name="optionText" format="string" />
        <attr name="optionExternalLink" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SettingsOptionsView">
        <attr name="settingName" format="string" />
        <attr name="settingValue" format="string" />
        <attr name="showChevron" format="boolean" />
